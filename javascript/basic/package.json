{
  "name": "discordjs-v13-handler",
  "version": "1.0.0",
  "description": "A basic javascript handler for discord.js v13.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon",
    "pm2:start": "pm2 start src/index.js --name djsbot --attach",
    "pm2:stop": "pm2 stop src/index.js",
    "pm2:restart": "pm2 stop src/index.js && pm2 start src/index.js --name djsbot --attach"
  },
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BulletTide/discordjs-v13-handlers.git"
  },
  "author": "Bullet_Tide",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BulletTide/discordjs-v13-handlers/issues"
  },
  "homepage": "https://github.com/BulletTide/discordjs-v13-handlers#readme",
  "dependencies": {
    "discord.js": "^13.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "pm2": "^5.1.1"
  }
}
